inal":{"line":133,"column":38}},{"name":"movies","generated":{"line":241,"column":20},"source":"components/main-view/main-view.jsx","original":{"line":133,"column":40}},{"generated":{"line":242,"column":0},"source":"components/main-view/main-view.jsx","original":{"line":133,"column":19}},{"generated":{"line":242,"column":12},"source":"components/main-view/main-view.jsx","original":{"line":133,"column":12}},{"generated":{"line":243,"column":0},"source":"components/main-view/main-view.jsx","original":{"line":133,"column":51}},{"generated":{"line":244,"column":0},"source":"components/main-view/main-view.jsx","original":{"line":132,"column":11}},{"generated":{"line":244,"column":8},"source":"components/main-view/main-view.jsx","original":{"line":105,"column":7}},{"generated":{"line":244,"column":9},"source":"components/main-view/main-view.jsx","original":{"line":104,"column":6}},{"generated":{"line":244,"column":10},"source":"components/main-view/main-view.jsx","original":{"line":103,"column":4}},{"generated":{"line":245,"column":0},"source":"components/main-view/main-view.jsx","original":{"line":138,"column":3}},{"name":"React","generated":{"line":249,"column":2},"source":"components/main-view/main-view.jsx","original":{"line":20,"column":30}},{"name":"Component","generated":{"line":249,"column":17},"source":"components/main-view/main-view.jsx","original":{"line":20,"column":36}},{"generated":{"line":249,"column":29},"source":"components/main-view/main-view.jsx","original":{"line":141,"column":0}},{"generated":{"line":250,"column":0},"source":"components/main-view/main-view.jsx","original":{"line":142,"column":0}},{"generated":{"line":251,"column":0},"source":"components/main-view/main-view.jsx","original":{"line":143,"column":0}},{"generated":{"line":252,"column":0},"source":"components/main-view/main-view.jsx","original":{"line":144,"column":0}},{"generated":{"line":253,"column":0},"source":"components/main-view/main-view.jsx","original":{"line":145,"column":0}},{"generated":{"line":254,"column":0},"source":"components/main-view/main-view.jsx","original":{"line":146,"column":0}},{"generated":{"line":255,"column":0},"source":"components/main-view/main-view.jsx","original":{"line":147,"column":0}},{"generated":{"line":256,"column":0},"source":"components/main-view/main-view.jsx","original":{"line":148,"column":0}},{"generated":{"line":257,"column":0},"source":"components/main-view/main-view.jsx","original":{"line":149,"column":0}},{"generated":{"line":258,"column":0},"source":"components/main-view/main-view.jsx","original":{"line":150,"column":0}},{"generated":{"line":259,"column":0},"source":"components/main-view/main-view.jsx","original":{"line":151,"column":0}},{"generated":{"line":260,"column":0},"source":"components/main-view/main-view.jsx","original":{"line":153,"column":1}}],"sources":{"components/main-view/main-view.jsx":"import React from 'react';\nimport axios from 'axios';\n\nimport { BrowserRouter as Router, Route, Switch} from \"react-router-dom\";\nimport { Link } from 'react-router-dom';\n\nimport { LoginView } from '../login-view/login-view';\nimport { RegistrationView } from '../registration-view/registration-view';\nimport { MovieCard } from '../movie-card/movie-card';\nimport { MovieView } from '../movie-view/movie-view';\nimport { DirectorView } from '../director-view/director-view';\nimport { GenreView } from '../genre-view/genre-view';\n//import { ProfileView } from '../profile-view/profile-view';\n\n// bootstrap import\nimport { Row, Col, Card, CardGroup, Nav, Navbar, Container } from 'react-bootstrap';\n\nimport './main-view.scss';\n\nexport class MainView extends React.Component {\n\n  constructor() {\n    super();\n\n    this.state = {\n      movies: [],\n  //    movie: null,\n  //    selectedMovie: null,\n      user: null\n    };\n  }\n  // old code from task 34 and before\n  // componentDidMount() {\n  //    axios.get('https://myflix-movie.herokuapp.com/movies')\n  //     .then(response => {\n  //       // Assign the result to the state\n  //       this.setState({\n  //         movies: response.data\n  //       });\n  //     })\n  //     .catch(function (error) {\n  //       console.log(error);\n  //     }); \n  // }\n\n  // // new code added with Task 3.5\n  componentDidMount() {\n    let accessToken = localStorage.getItem('token');\n    if (accessToken !== null) {\n      this.setState({\n        user: localStorage.getItem('user')\n      });\n      this.getMovies(accessToken);\n    }\n  }\n\n  \n  // // new method get movies, new code Task 3.5, make a request to the movies endpoint\n  getMovies(token) {\n    axios.get('https://myflix-movie.herokuapp.com/movies', {\n      headers: { Authorization: `Bearer ${token}`}\n     })\n    .then(response => {\n       // Assign the result to the state\n       this.setState({\n       movies: response.data\n      });\n   })\n   .catch(function (error) {\n   console.log(error);\n   });\n  }\n\n  onLoggedIn(authData) {\n    console.log(authData);\n    this.setState({\n      user: authData.user.Username\n    });\n  \n    localStorage.setItem('token', authData.token);\n    localStorage.setItem('user', authData.user.Username);\n    this.getMovies(authData.token);\n  }\n\n// new method for signing out, button mainview\n  onLoggedOut() {\n    this.setState({\n      user: null\n    });\n    localStorage.removeItem(\"token\");\n    localStorage.removeItem(\"user\");\n    window.open('/', '_self');\n  }\n\n\n  render() {\n    const { movies, user } = this.state;\n    const username = localStorage.getItem('user');\n\n   if (!user) return <LoginView onLoggedIn={user => this.onLoggedIn(user)} />;\n    if (!movies) return <div className=\"main-view\"/>;\n\n    return (\n      <Router>\n       <CardGroup className=\"main-view\">\n         <Navbar bg=\"success\" variant=\"dark\" fixed=\"top\">\n         <Navbar.Brand as={Link} to=\"/\">MyFlix Movie</Navbar.Brand>\n            <Nav className=\"mr-auto\">\n                <Nav.Link as={Link} to=\"/\">Home</Nav.Link>\n                <Nav.Link href={`/client/profile/${user}`}>Account</Nav.Link>\n                <Nav.Link onClick={(user) => this.onLoggedOut()} href=\"/client/\">\n\t\t\t\t\t\t\t\t\t\tLogout\n\t\t\t\t\t\t\t\t</Nav.Link>\n            </Nav>\n          </Navbar>\n          <Route exact path=\"/\" render={() => {\n            if (!user) return <LoginView onLoggedIn={user => this.onLoggedIn(user)} />;\n            return movies.map(m => <MovieCard key={m._id} movie={m}/>)\n           }\n          }/>\n          <Route path=\"/register\" render={() => <RegistrationView />} />\n           <Route path=\"/movies/:movieId\" \n              render={({match}) => <MovieView movie={movies.find(m => m._id === match.params.movieId)}/>}/>\n          <Route path=\"/genres/:name\" render={({ match }) => {\n            if (!movies) return <CardGroup className=\"main-view\"/>;\n            return <GenreView genre={movies.find(m => m.Genre.Name === match.params.name).Genre}/>}\n           } />\n           <Route path=\"/directors/:name\" render={({ match }) => {\n              if (!movies) return <CardGroup className=\"main-view\"/>;\n             return <DirectorView director={movies.find(m => m.Director.Name === match.params.name).Director}/>}\n           } />\n           <Route exact path=\"/users/:Username\" render={() => {\n            return <ProfileView movies={movies} />}\n            } />\n        </CardGroup>\n      </Router>\n    );\n  }\n }\n\n// using \n//  <Switch>\n//  <Route exact path=\"/\" render={() => {\n//    if (!user) return <LoginView onLoggedIn={user => this.onLoggedIn(user)} />;\n//    return movies.map(m => <MovieCard key={m._id} movie={m}/>)\n//    }\n//    }/>\n//     <Route path=\"/\" exact={true}>\n//       <RegistrationForm />\n//     </Route>\n//   </Switch>\n\n // home link: <Nav.Link as={Link} to=\"/\">Home</Nav.Link>\n"},"lineCount":null}},"error":null,"hash":"4ce3ee2648c90c59ef3c517ebf2e2f4e","cacheData":{"env":{}}}
